    "personalization-depth": {
      "id": "exp_personalization_001",
      "name": "Personalization Depth Testing",
      "description": "Testing different levels of email personalization effectiveness",
      "status": "active",
      "target_audience": "physician",
      "start_date": "2025-02-01",
      "end_date": "2025-05-01",
      "sample_size": 1500,
      "confidence_level": 95,
      "statistical_power": 80,
      "primary_metric": "engagement_score",
      "secondary_metrics": ["read_time", "click_through_rate", "reply_rate"],
      "variations": {
        "basic": {
          "name": "Basic Personalization (Name only)",
          "weight": 25,
          "personalization_fields": ["FNAME"],
          "dynamic_content": false,
          "behavioral_triggers": false
        },
        "moderate": {
          "name": "Moderate Personalization (Name + Specialty)",
          "weight": 25,
          "personalization_fields": ["FNAME", "SPECIALTY", "PRACTICE_MODEL"],
          "dynamic_content": true,
          "behavioral_triggers": false
        },
        "advanced": {
          "name": "Advanced Personalization (Full Profile)",
          "weight": 25,
          "personalization_fields": ["FNAME", "SPECIALTY", "PRACTICE_MODEL", "CHALLENGE", "EMR_SYSTEM", "PRACTICE_SIZE"],
          "dynamic_content": true,
          "behavioral_triggers": true
        },
        "hyper": {
          "name": "Hyper-personalized (AI-generated content)",
          "weight": 25,
          "personalization_fields": ["ALL_AVAILABLE"],
          "dynamic_content": true,
          "behavioral_triggers": true,
