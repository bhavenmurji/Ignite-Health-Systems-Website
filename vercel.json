{
  "buildCommand": "cd ignite-health-systems && npm run build",
  "outputDirectory": "ignite-health-systems/.next",
  "framework": "nextjs",
  "installCommand": "cd ignite-health-systems && npm install",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|webp|css|js)$)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, stale-while-revalidate=60"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}